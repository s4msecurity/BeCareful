{
  "manifest_version": 2,
  "name": "BeCareful",
  "version": "1.0",
  "description": "Warning system for Phising attack.",
  "icons": {
    "48": "icons/icon.jpg"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content_script/main.js"]
    }
  ], 
  "browser_action": {
    "browser_style": true,
    "default_icon": "icons/icon.jpg",
    "default_title": "BeCareful: Phising detection system."
  }
}